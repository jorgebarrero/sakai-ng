{
  "name": "sakai-ng",
  "version": "14.0.0",
  "scripts": {
    "ng": "ng",
    "postinstall": "",
    "start": "node git-version.js; ng serve --live-reload=false",
    "start:dev": "reset && node git-version.js && ng serve --live-reload=false",
    "start:db": "json-server --watch src/shared/data/db.json",
    "build": "node git-version.js && ng build --output-path=dist && node git-version.js",
    "build:prod": "ng build --output-path=dist && node git-version.js",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "git-flow": "git flow init",
    "kill:port": "kill -9 $(lsof -t -i:4200)",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "post-build": "node ./build/post-build.js",
    "build:stats": "ng build --stats-json",
    "analyze": "webpack-bundle-analyzer dist/stats.json",
    "git:fix": "git rm -r --cached .; git add .; git commit -m 'Untracked files issue resolved to fix .gitignore'; ",
    "generate-icons": "svg-generator"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~14.1.0",
    "@angular/cdk": "~14.0.0",
    "@angular/common": "~14.1.0",
    "@angular/compiler": "~14.1.0",
    "@angular/core": "~14.1.0",
    "@angular/forms": "~14.1.0",
    "@angular/localize": "^14.1.0",
    "@angular/platform-browser": "~14.1.0",
    "@angular/platform-browser-dynamic": "~14.1.0",
    "@angular/router": "~14.1.0",
    "bootstrap": "^5.1.3",
    "chart.js": "^3.3.2",
    "echarts": "^5.3.3",
    "file-saver": "^2.0.5",
    "fs-extra": "^8.1.0",
    "git-describe": "^4.1.0",
    "leaflet": "^1.8.0",
    "moment-timezone": "^0.5.34",
    "ngx-echarts": "^8.0.0",
    "number-format.js": "^2.0.9",
    "primeflex": "^3.2.0",
    "primeicons": "5.0.0",
    "primeng": "14.0.0",
    "prismjs": "1.9.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~14.1.0",
    "@angular/cli": "~14.1.0",
    "@angular/compiler-cli": "~14.1.0",
    "@types/file-saver": "^2.0.5",
    "@types/jasmine": "~3.10.0",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.10.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.7.2"
  }
}
